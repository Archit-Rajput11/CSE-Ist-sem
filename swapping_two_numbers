#include<stdio.h>30
int main(){
    int a,b,temp;
    printf("Enter first number (a):");
    scanf("%d",&a);
    printf("Enter second number (b):");
    scanf("%d",&b);
    printf("Before swapping a=%d\nb=%d\n",a,b);
    temp=a;
    a=b;
    b=temp;
    printf("after swapping a=%d\nb=%d",a,b);
    return 0;

}
